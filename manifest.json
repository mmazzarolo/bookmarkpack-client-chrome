{
  "manifest_version": 2,

  "name": "Bookmarkpack",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "web_accessible_resources": [
    "img/icon128.png"
  ]
}
